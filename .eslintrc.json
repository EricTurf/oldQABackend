{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": ["react", "import", "prettier"],
  "env": {
    "jasmine": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "comma-dangle": "error",
    "linebreak-style": "off",
    "react/jsx-uses-vars": [2],
    "prettier/prettier": "error"
  }
}
